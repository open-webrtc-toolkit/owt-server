{
    "name": "sip-agent",
    "dest": "sip_agent",
    "common": {
        "package": "../package.json",
        "files": [
            "agent.toml",
            "log4js_configuration.json",
            "install_deps.sh",
            "../index.js",
            "../erizoJS.js",
            "../log4cxx.properties",
            "../../common/amqp_client.js",
            "../../common/clusterWorker.js",
            "../../common/loadCollector.js",
            "../../common/logger.js",
            "../../common/makeRPC.js",
            "../../common/rpcChannel.js"
        ],
        "folders": {
            "sip": [
                "index.js",
                "sipCallConnection.js",
                "../connections.js",
                "../InternalConnectionFactory.js"
            ]
        }
    },
    "natives": {
        "libdist": "lib",
        "addons": [
            {
                "folder": "sipIn",
                "name": "sipIn"
            },
            {
                "folder": "../addons/internalIO",
                "name": "internalIO"
            },
            {
                "folder": "../addons/logger",
                "name": "logger"
            }
        ]
    },
    "start": "node . -U sip"
}