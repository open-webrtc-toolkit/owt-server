{
  "appenders": {
    "out": {
      "type": "console",
      "layout": {
        "type": "pattern",
        "pattern": "%d  - %p: %c - %m",
        "replaceConsole": true
      }
    }
  },
  "categories": {
    "default": { "appenders": [ "out" ], "level": "info" },
    "ClusterWorker": { "appenders": [ "out" ], "level": "info" },
    "LoadCollector": { "appenders": [ "out" ], "level": "info" },
    "AmqpClient": { "appenders": [ "out" ], "level": "info" },
    "WorkingJS": { "appenders": [ "out" ], "level": "info" },
    "NodeManager": { "appenders": [ "out" ], "level": "info" },
    "AnalyticsNode": { "appenders": [ "out" ], "level": "info" },
    "AnalyticsAgent": { "appenders": [ "out" ], "level": "warn" },
    "Connections": { "appenders": [ "out" ], "level": "info" },
    "InternalConnectionRouter": { "appenders": [ "out" ], "level": "info" }
  }
}
