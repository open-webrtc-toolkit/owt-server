{
    "name": "video-agent",
    "dest": "video_agent",
    "common": {
        "package": "../package.json",
        "files": [
            "agent.toml",
            "log4js_configuration.json",
            "init.sh",
            "install_deps.sh",
            "install_openh264.sh",
            "uninstall_openh264.sh",
