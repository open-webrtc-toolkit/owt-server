            "../../common/amqp_client.js",
            "../../common/clusterWorker.js",
            "../../common/loadCollector.js",
            "../../common/logger.js",
            "../../common/makeRPC.js",
            "../../common/rpcChannel.js",
            "../../common/mediaUtil.js",
            "../../../scripts/release/install_ffmpeg.sh",
            "../../../scripts/release/initauth.js",
            "../../../scripts/detectOS.sh",
            "compile_svtHevcEncoder.sh"
