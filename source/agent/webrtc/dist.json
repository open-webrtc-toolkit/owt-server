{
    "name": "webrtc-agent",
    "dest": "webrtc_agent",
    "node": {
        "bin": "woogeen_webrtc",
        "entry": "../erizoJS.js",
        "libpath": "webrtc",
        "libfiles": [
            "index.js",
            "wrtcConnection.js",
            "quicConnection.js",
            "../connections.js",
            "../InternalConnectionFactory.js"
        ]
    },
    "common": {
        "package": "package.json",
        "files": [
            "agent.toml",
            "log4js_configuration.json",
            "install_deps.sh",
            "configLoader.js",
            "../index.js",
            "../networkHelper.js",
            "../nodeManager.js",
            "../workingNode.js",
            "log4cxx.properties",
            "../../common/amqp_client.js",
            "../../common/cipher.js",
            "../../common/clusterWorker.js",
            "../../common/loadCollector.js",
            "../../common/logger.js",
            "../../common/makeRPC.js",
            "../../common/rpcChannel.js",
            "../../../scripts/release/initcert.js",
            "../../../scripts/release/initauth.js",
            "../../../scripts/detectOS.sh"
        ],
        "folders": {
            "webrtc": [
                "index.js",
                "wrtcConnection.js",
                "connection.js",
                "mediaConfig.js",
                "profileFilter.js",
                "sdpInfo.js",
                "../connections.js",
                "../InternalConnectionFactory.js"
            ],
            "cert": [
                "../../../cert/.owt.keystore"
            ]
        }
    },
    "natives": {
        "libdist": "lib",
        "addons": [
            {
                "folder": "rtcConn",
                "name": "rtcConn"
            },
            {
                "folder": "rtcFrame",
                "name": "rtcFrame"
            },
            {
                "folder": "../addons/internalIO",
                "name": "internalIO"
            },
            {
                "folder": "../addons/logger",
                "name": "logger"
            },
            {
                "folder": "../addons/quicIO",
                "name": "quicIO"
            }
        ]
    },
    "start": "node . -U webrtc"
}
