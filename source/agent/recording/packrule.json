{
    "name": "recording-agent",
    "dest": "recording_agent",
    "node": {
        "bin": "woogeen_recording",
        "entry": "../erizoJS.js",
        "libpath": "recording",
        "libfiles": [
            "index.js",
            "../connections.js",
            "../InternalConnectionFactory.js"
        ]
    },
    "common": {
        "package": "../package.json",
        "files": [
            "../AgentLoader.js",
            "agent.toml",
            "log4js_configuration.json",
            "install_deps.sh",
            "../index.js",
            "../log4cxx.properties",
            "../../common/amqp_client.js",
            "../../common/clusterWorker.js",
            "../../common/loadCollector.js",
            "../../common/logger.js",
            "../../common/makeRPC.js",
            "../../common/rpcChannel.js",
            "../../../scripts/detectOS.sh"
        ]
    },
    "natives": {
        "libdist": "lib",
        "addons": [
            {
                "folder": "../addons/avstreamLib",
                "name": "avstream"
            },
            {
                "folder": "../addons/internalIO",
                "name": "internalIO"
            },
            {
                "folder": "../addons/logger",
                "name": "logger"
            }
        ]
    },
    "start": "node . -U recording"
}